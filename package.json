      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openzeppelin/truffle-upgrades": "^1.11.0",
    "@storybook/addon-actions": "^6.2.8",
